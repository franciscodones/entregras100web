                                    "        } else {",
                                    "            promesaPermisosLoad.reject();",
                                    "        }",
                                    "    });",
                                    "    usuariosLocalStore.load(function(records, operation, success) {",
                                    "        if (success) {",
                                    "            promesaUsuariosLoad.resolve();",
                                    "        } else {",
                                    "            promesaUsuariosLoad.reject();",
                                    "        }",
                                    "    });",
                                    "    tiposSesionLocalStore.load(function(records, operation, success) {",
                                    "        if (success) {",
